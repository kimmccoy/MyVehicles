{
  "name": "my-vehicles-client",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --deploy-url /dist/",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "generate-api": "ng-openapi -i ./openapi/v1.json -o ./src/api"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@9.3.0",
  "dependencies": {
    "@angular/common": "^21.0.0",
    "@angular/compiler": "^21.0.0",
    "@angular/core": "^21.0.0",
    "@angular/forms": "^21.0.0",
    "@angular/platform-browser": "^21.0.0",
    "@angular/router": "^21.0.0",
    "@microsoft/signalr": "^10.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^21.0.1",
    "@angular/cli": "^20.0.6",
    "@angular/compiler-cli": "^21.0.0",
    "jsdom": "^27.1.0",
    "ng-openapi": "^0.2.9",
    "typescript": "~5.9.2",
    "vitest": "^4.0.8"
  }
}
